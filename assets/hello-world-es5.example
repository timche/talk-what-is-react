var Hello = React.createClass({
  displayName: 'Hello',

  render: function render() {
    return React.createElement(
      'h1',
      null,
      'Hello ',
      this.props.name
    )
  }
})

ReactDOM.render(
  React.createElement(
    Hello,
    { name: 'John' }
  ),
  document.getElementById('root')
)
